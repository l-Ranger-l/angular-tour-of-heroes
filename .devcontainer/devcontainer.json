{
    "name": "Tour of Heroes",
    "dockerFile": "Dockerfile",
    "appPort": [3000],
    "runArgs": ["-u", "node"],
    "settings": {
        "workbench.colorTheme": "Default Dark+",
        "terminal.integrated.profiles.linux": {
            "bash": {
                "path": "bash",
                "icon": "terminal-bash"
            },
        }
    },
    "postCreateCommand": "npm install npm@8.5.5",
    "extensions": [
        "esbenp.prettier-vscode"
    ]

}